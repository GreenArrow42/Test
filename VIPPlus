package net.VIPPlus;

import java.io.File;
import java.io.IOException;
import java.io.InputStream;
import java.util.HashMap;
import java.util.List;

import org.bukkit.Bukkit;
import org.bukkit.ChatColor;
import org.bukkit.Effect;
import org.bukkit.Sound;
import org.bukkit.command.Command;
import org.bukkit.command.CommandSender;
import org.bukkit.configuration.file.FileConfiguration;
import org.bukkit.configuration.file.YamlConfiguration;
import org.bukkit.entity.Player;
import org.bukkit.event.Listener;
import org.bukkit.potion.PotionEffect;
import org.bukkit.potion.PotionEffectType;
import org.bukkit.plugin.PluginManager;
import org.bukkit.plugin.java.JavaPlugin;

public class VIPPlus extends JavaPlugin implements Listener {
	
	
	public static FileConfiguration Yapilandirma = null;
	public static File Dosya = null;
	public static YamlConfiguration Dil = null;
	public static File Dil_Dosyasi = null;
	Guncelleyici Guncelleme = new Guncelleyici(this);
	public String Adres = "http://yadi.sk/d/S-c1eAqxLsAfg";
	public String GuncellemeYolu = "plugins" + File.separator + "VIPPlus.jar";
	public static YamlConfiguration VarsayilanYapilandirma;
	public HashMap<String, Long> Rejenerasyon_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Ceviklik_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Ates_Direnci_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Iyilestirme_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Gece_Gorus_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Gorunmezlik_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Ziplama_Destegi_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Direnc_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Kuvvet_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Acele_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Su_Altinda_Nefes_Alma_Bekleme_Suresi = new HashMap<String, Long>();
	public HashMap<String, Long> Saglik_Arttirici_Bekleme_Suresi = new HashMap<String, Long>();
	
	public void onEnable() {
	    PluginManager EklentiYoneticisi = Bukkit.getPluginManager();
	    EklentiYoneticisi.registerEvents(this, this);
	    getLogger().info(getDescription().getName() + " Versiyon " + getDescription().getVersion() + " Aktif Edildi!");
	    getLogger().info("Guncellemeler Kontrol Ediliyor...");
	    if(Guncelleme.GuncellemeKontrol().booleanValue()) {
	    	EklentiYoneticisi.disablePlugin(this);
	    	for (int i = 0; i < 20; i++) {
	    		getLogger().info(getDescription().getName() + " Guncelleme Basarili! " + getDescription().getName() + " Versiyon " + getDescription().getVersion() + " Etkinlestirmek Icin Sunucuyu Yeniden Baslatmaniz Gerekiyor!");
	    		EklentiYoneticisi.disablePlugin(this);
	    		EklentiYoneticisi.enablePlugin(this);
	    	}
	    }
		YapilandirmaYukle();
		DilDosyasiniYukle();
	}
	
	public void onDisable() {
		
		getLogger().info(getDescription().getName() + " Versiyon " + getDescription().getVersion() + " Pasif Durumda!");
	}
	
	public void YapilandirmaYukle() {
		if (Dosya == null || Yapilandirma == null) {
		Yapilandirma = getConfig();
		Yapilandirma.options().copyDefaults(true);
		Dosya = new File(getDataFolder(), "config.yml");
		if (!Dosya.exists()) {
		try {
			Dosya.mkdir();
			Yapilandirma.save(Dosya);
		} catch (IOException Istisna) {
			getServer().getConsoleSender().sendMessage("[" + getDescription().getName() + "] " + ChatColor.RED + Dosya +" Dosyasi Kaydedilemedi!");
			Istisna.printStackTrace();
		}
		}
		}
	}
	
	public void YapilandirmaYenile() {
		Yapilandirma = YamlConfiguration.loadConfiguration(Dosya);
		Dil = YamlConfiguration.loadConfiguration(Dil_Dosyasi);
		reloadConfig();
		saveConfig();
		PluginManager EklentiYoneticisi = Bukkit.getPluginManager();
		VIPPlus EklentiAdi = (VIPPlus) EklentiYoneticisi.getPlugin("VIPPlus");
		EklentiYoneticisi.disablePlugin(EklentiAdi);
		EklentiYoneticisi.enablePlugin(EklentiAdi);
        reloadConfig();
        saveConfig();
		YapilandirmaYukle();
		DilDosyasiniYukle();
	}
	
	public void DilDosyasiniYukle() {
		
		File dilDosyasi = new File(getDataFolder(), "mesajlar.lang");
		if (!dilDosyasi.exists()) {
			try {
				getDataFolder().mkdir();
				dilDosyasi.createNewFile();
				InputStream TanimliYapilandirmaIzle = this.getResource("mesajar.lang");
				if (TanimliYapilandirmaIzle != null) {
					VarsayilanYapilandirma = YamlConfiguration.loadConfiguration(TanimliYapilandirmaIzle);
					VarsayilanYapilandirma.save(dilDosyasi);
					DilDosyasi.DosyaKoy(VarsayilanYapilandirma);
					VarsayilanYapilandirma();
					
				}
			} catch(IOException Istisna) {
				Istisna.printStackTrace();
				getServer().getConsoleSender().sendMessage("[" + getDescription().getName() + "]" + ChatColor.RED + " Dil Dosyasi Olusturulamadi!");
				getServer().getConsoleSender().sendMessage("[" + getDescription().getName() + "]" + ChatColor.AQUA + " Hatadan Dolayi Eklenti Pasif Birakiliyor.");
				this.setEnabled(false);
			}
		}
		YamlConfiguration yapilandirma = YamlConfiguration.loadConfiguration(dilDosyasi);
		for (DilDosyasi Oge : DilDosyasi.values()) {
			if (yapilandirma.getString(Oge.YolOlma()) == null) {
				yapilandirma.set(Oge.YolOlma(), Oge.VarsayilanOlma());
			}
		}
		DilDosyasi.DosyaKoy(yapilandirma);
		VIPPlus.Dil = yapilandirma;
		VIPPlus.Dil_Dosyasi = dilDosyasi;
		try {
			yapilandirma.save(DilDosyasiOlma());
		} catch(IOException Istisna) {
			getServer().getConsoleSender().sendMessage("[" + getDescription().getName() + "]" + ChatColor.RED + " plugins/VIPPlus/mesajlar.lang Dosyasi Kaydedilemedi!");
	        Istisna.printStackTrace();
		}
	}
	
	public YamlConfiguration DilOlma() {
		
		return Dil;
	}
	
	public File DilDosyasiOlma() {
		
		return Dil_Dosyasi;
	}
	
	public YamlConfiguration VarsayilanYapilandirma() {
		
		return VarsayilanYapilandirma;
	}
	
	public boolean onCommand(CommandSender Gonderen, Command Komut, String KomutEtiketi, String[] Arguman) {
		Player Oyuncu = (Player) Gonderen;
		if (Komut.getName().equalsIgnoreCase("vipplus")) {
        	if (Oyuncu.hasPermission("vipplus.anakomutmesaji")) {
			if (Arguman.length == 0) {
				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Ana_Komut_Mesaji);
				return true;
			}
        	}
			else {
				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
				return true;
			}
            if (Arguman.length == 1) {
            	if (Arguman[0].equalsIgnoreCase("yardimlistesi") || Arguman[0].equalsIgnoreCase("yl")) {
            	if (Oyuncu.hasPermission("vipplus.yardimlistesi")) {
                	if (Yapilandirma.getBoolean("Genel_Ayarlar.Yardim_Listesi", true)) {
            		List<String> Yardim_Listesi = VIPPlus.Yapilandirma.getStringList("Yardim_Listesi");
            		for (String Dize : Yardim_Listesi)
            			Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + Dize.replace('&', 'ยง'));
            		return true;
            	}
            	}
    			else {
    				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
    				return true;
    			}
               }
            }
            if (Arguman.length == 1) {
            	if (Arguman[0].equalsIgnoreCase("komutlistesi") || Arguman[0].equalsIgnoreCase("kl")) {
            	if (Oyuncu.hasPermission("vipplus.komutlistesi")) {
                	if (Yapilandirma.getBoolean("Genel_Ayarlar.Komut_Listesi", true)) {
            		List<String> Komut_Listesi = VIPPlus.Yapilandirma.getStringList("Komut_Listesi");
            		for (String Dize : Komut_Listesi)
            			Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + Dize.replace('&', 'ยง'));
            		return true;
            	}
            	}
    			else {
    				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
    				return true;
    			}
               }
            }
            if (Arguman.length == 1) {
                if (Arguman[0].equalsIgnoreCase("yenile") || Arguman[0].equalsIgnoreCase("y")) {
                	if (Oyuncu.hasPermission("vipplus.yenile")) {
                    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Eklenti_Dosyalari_Yeniden_Yukleniyor_Mesaji);
                	YapilandirmaYenile();
                	Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Eklenti_Dosyalari_Yeniden_Yuklendi_Mesaji);
                	}
        			else { Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji); }
                    return true;
                }
            }
            if (Arguman.length == 1) {
                if (Arguman[0].equalsIgnoreCase("pasif") || Arguman[0].equalsIgnoreCase("p")) {
                	if (Oyuncu.hasPermission("vipplus.pasif")) {
                    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Oyun_Ici_Eklenti_Pasif_Mesaji);
                	Bukkit.getServer().getPluginManager().disablePlugin(this);
                	}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
                }
            }
            if (Arguman.length == 1) {
            	if (Arguman[0].equalsIgnoreCase("sut") || Arguman[0].equalsIgnoreCase("s")) {
            		if (Oyuncu.hasPermission("vipplus.sut")) {
            			for (PotionEffect Iksir_Etkisi : Oyuncu.getActivePotionEffects()) {
            				Oyuncu.removePotionEffect(Iksir_Etkisi.getType());
            				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Iksir_Etkisi_Kaldirildi_Mesaji);
            			}
            		}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
            	}
            }
            if (Arguman.length == 1) {
                long Rejenerasyon_Bekleme_Suresi;
                Rejenerasyon_Bekleme_Suresi = Yapilandirma.getInt("Rejenerasyon.Bekleme_Suresi");
            	if (Yapilandirma.getBoolean("Rejenerasyon.Rejenerasyon", true))
                if (Arguman[0].equalsIgnoreCase("rejenerasyon") || Arguman[0].equalsIgnoreCase("r")) {
                	if (Oyuncu.hasPermission("vipplus.rejenerasyon")) {
            	    if (this.Rejenerasyon_Bekleme_Suresi.containsKey(Oyuncu.getName())) {
            	    	if (!Oyuncu.isOp()) {
            	        Rejenerasyon_Bekleme_Suresi = ((Long)this.Rejenerasyon_Bekleme_Suresi.get(Oyuncu.getName())).longValue() / 1000L + Rejenerasyon_Bekleme_Suresi - (System.currentTimeMillis() / 1000L);
            	        if (Rejenerasyon_Bekleme_Suresi > 0L) {
            	        	if (Yapilandirma.getBoolean("Rejenerasyon.Bekleme_Sure_Mesaji", true)) {
            	          Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + ChatColor.RED + "Rejenerasyon Kullanabilmek Icin Lutfen " + ChatColor.GREEN + Rejenerasyon_Bekleme_Suresi + ChatColor.RED + " Saniye Bekleyiniz!");
            	        	}
              	          return true;
            	        }
            	    }
            	    }
            	    this.Rejenerasyon_Bekleme_Suresi.put(Oyuncu.getName(), Long.valueOf(System.currentTimeMillis()));
            	    if (Yapilandirma.getBoolean("Rejenerasyon.Etki_Mesaji", true)) {
            	    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Rejenerasyon_Etkin_Mesaji);
            	    }
                	if (Yapilandirma.getBoolean("Rejenerasyon.Efekt", true)) {
                	Oyuncu.getWorld().playEffect(Oyuncu.getPlayer().getLocation(), Effect.valueOf(Yapilandirma.getString("Rejenerasyon.Efekt_Cesiti")), 1);
                	}
                	if (Yapilandirma.getBoolean("Rejenerasyon.Ses_Efekt", true)) {
                	Oyuncu.getWorld().playSound(Oyuncu.getPlayer().getLocation(), Sound.valueOf(Yapilandirma.getString("Rejenerasyon.Ses_Efekti")), Yapilandirma.getInt("Rejenerasyon.Ses_Etki_Alani"), Yapilandirma.getInt("Rejenerasyon.Ses_Seviyesi"));
                	}
            		Oyuncu.addPotionEffect(new PotionEffect(PotionEffectType.REGENERATION, Yapilandirma.getInt("Rejenerasyon.Etki_Suresi"), Yapilandirma.getInt("Rejenerasyon.Etki_Gucu")));
                	}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
                }
            }
            if (Arguman.length == 1) {
                long Ceviklik_Bekleme_Suresi;
                Ceviklik_Bekleme_Suresi = Yapilandirma.getInt("Ceviklik.Bekleme_Suresi");
            	if (Yapilandirma.getBoolean("Ceviklik.Ceviklik", true))
                if (Arguman[0].equalsIgnoreCase("ceviklik") || Arguman[0].equalsIgnoreCase("c")) {
                	if (Oyuncu.hasPermission("vipplus.ceviklik")) {
            	    if (this.Ceviklik_Bekleme_Suresi.containsKey(Oyuncu.getName())) {
            	    	if (!Oyuncu.isOp()) {
            	    	Ceviklik_Bekleme_Suresi = ((Long)this.Ceviklik_Bekleme_Suresi.get(Oyuncu.getName())).longValue() / 1000L + Ceviklik_Bekleme_Suresi - (System.currentTimeMillis() / 1000L);
            	        if (Ceviklik_Bekleme_Suresi > 0L) {
            	        	if (Yapilandirma.getBoolean("Ceviklik.Bekleme_Sure_Mesaji", true)) {
            	          Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + ChatColor.RED + "Cevikligi Kullanabilmek Icin Lutfen " + ChatColor.GREEN + Ceviklik_Bekleme_Suresi + ChatColor.RED + " Saniye Bekleyiniz!");
            	        	}
              	          return true;
            	        }
            	    }
            	    }
            	    this.Ceviklik_Bekleme_Suresi.put(Oyuncu.getName(), Long.valueOf(System.currentTimeMillis()));
            	    if (Yapilandirma.getBoolean("Ceviklik.Etki_Mesaji", true)) {
            	    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Ceviklik_Etkin_Mesaji);
            	    }
                	if (Yapilandirma.getBoolean("Ceviklik.Efekt", true)) {
                    	Oyuncu.getWorld().playEffect(Oyuncu.getPlayer().getLocation(), Effect.valueOf(Yapilandirma.getString("Ceviklik.Efekt_Cesiti")), 1);
                    	}
                    	if (Yapilandirma.getBoolean("Ceviklik.Ses_Efekt", true)) {
                    	Oyuncu.getWorld().playSound(Oyuncu.getPlayer().getLocation(), Sound.valueOf(Yapilandirma.getString("Ceviklik.Ses_Efekti")), Yapilandirma.getInt("Ceviklik.Ses_Etki_Alani"), Yapilandirma.getInt("Ceviklik.Ses_Seviyesi"));
                	}
                    Oyuncu.addPotionEffect(new PotionEffect(PotionEffectType.SPEED, Yapilandirma.getInt("Ceviklik.Etki_Suresi"), Yapilandirma.getInt("Ceviklik.Etki_Gucu")));
                	}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
                }
            }
            if (Arguman.length == 1) {
                long Ates_Direnci_Bekleme_Suresi;
                Ates_Direnci_Bekleme_Suresi = Yapilandirma.getInt("Ayarlar.Bekleme_Suresi");
            	if (Yapilandirma.getBoolean("Ates_Direnci.Ates_Direnci", true))
                if (Arguman[0].equalsIgnoreCase("atesdirenci") || Arguman[0].equalsIgnoreCase("ad")) {
                	if (Oyuncu.hasPermission("vipplus.atesdirenci")) {
            	    if (this.Ates_Direnci_Bekleme_Suresi.containsKey(Oyuncu.getName())) {
            	    	if (!Oyuncu.isOp()) {
            	    	Ates_Direnci_Bekleme_Suresi = ((Long)this.Ates_Direnci_Bekleme_Suresi.get(Oyuncu.getName())).longValue() / 1000L + Ates_Direnci_Bekleme_Suresi - (System.currentTimeMillis() / 1000L);
            	        if (Ates_Direnci_Bekleme_Suresi > 0L) {
            	        	if (Yapilandirma.getBoolean("Ates_Direnci.Bekleme_Sure_Mesaji", true)) {
            	          Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + ChatColor.RED + "Ates Direncini Kullanabilmek Icin Lutfen " + ChatColor.GREEN + Ates_Direnci_Bekleme_Suresi + ChatColor.RED + " Saniye Bekleyiniz!");
            	        	}
              	          return true;
            	        }
            	    }
            	    }
            	    this.Ates_Direnci_Bekleme_Suresi.put(Oyuncu.getName(), Long.valueOf(System.currentTimeMillis()));
            	    if (Yapilandirma.getBoolean("Ates_Direnci.Etki_Mesaji", true)) {
            	    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Ates_Direnci_Etkin_Mesaji);
            	    }
                	if (Yapilandirma.getBoolean("Ates_Direnci.Efekt", true)) {
                    	Oyuncu.getWorld().playEffect(Oyuncu.getPlayer().getLocation(), Effect.valueOf(Yapilandirma.getString("Ates_Direnci.Efekt_Cesiti")), 1);
                    	}
                    	if (Yapilandirma.getBoolean("Ayarlar.Ses_Efekt", true)) {
                    	Oyuncu.getWorld().playSound(Oyuncu.getPlayer().getLocation(), Sound.valueOf(Yapilandirma.getString("Ates_Direnci.Ses_Efekti")), Yapilandirma.getInt("Ates_Direnci.Ses_Etki_Alani"), Yapilandirma.getInt("Ates_Direnci.Ses_Seviyesi"));
                	}
                    Oyuncu.addPotionEffect(new PotionEffect(PotionEffectType.FIRE_RESISTANCE, Yapilandirma.getInt("Ates_Direnci.Etki_Suresi"), Yapilandirma.getInt("Ates_Direnci.Etki_Gucu")));
                	}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
                }
            }
            if (Arguman.length == 1) {
                long Iyilestirme_Bekleme_Suresi;
                Iyilestirme_Bekleme_Suresi = Yapilandirma.getInt("Iyilestirme.Bekleme_Suresi");
            	if (Yapilandirma.getBoolean("Iyilestirme.Iyilestirme", true))
                if (Arguman[0].equalsIgnoreCase("iyilestirme") || Arguman[0].equalsIgnoreCase("i")) {
                	if (Oyuncu.hasPermission("vipplus.iyilestirme")) {
            	    if (this.Iyilestirme_Bekleme_Suresi.containsKey(Oyuncu.getName())) {
            	    	if (!Oyuncu.isOp()) {
            	    	Iyilestirme_Bekleme_Suresi = ((Long)this.Iyilestirme_Bekleme_Suresi.get(Oyuncu.getName())).longValue() / 1000L + Iyilestirme_Bekleme_Suresi - (System.currentTimeMillis() / 1000L);
            	        if (Iyilestirme_Bekleme_Suresi > 0L) {
            	        	if (Yapilandirma.getBoolean("Iyilestirme.Bekleme_Sure_Mesaji", true)) {
            	          Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + ChatColor.RED + "Iyilestirme Kullanabilmek Icin Lutfen " + ChatColor.GREEN + Iyilestirme_Bekleme_Suresi + ChatColor.RED + " Saniye Bekleyiniz!");
            	        	}
              	          return true;
            	        }
            	    }
            	    }
            	    this.Iyilestirme_Bekleme_Suresi.put(Oyuncu.getName(), Long.valueOf(System.currentTimeMillis()));
            	    if (Yapilandirma.getBoolean("Iyilestirme.Etki_Mesaji", true)) {
            	    Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Iyilestirme_Etkin_Mesaji);
            	    }
                	if (Yapilandirma.getBoolean("Iyilestirme.Efekt", true)) {
                    	Oyuncu.getWorld().playEffect(Oyuncu.getPlayer().getLocation(), Effect.valueOf(Yapilandirma.getString("Iyilestirme.Efekt_Cesiti")), 1);
                    	}
                    	if (Yapilandirma.getBoolean("Iyilestirme.Ses_Efekt", true)) {
                    	Oyuncu.getWorld().playSound(Oyuncu.getPlayer().getLocation(), Sound.valueOf(Yapilandirma.getString("Iyilestirme.Ses_Efekti")), Yapilandirma.getInt("Iyilestirme.Ses_Etki_Alani"), Yapilandirma.getInt("Iyilestirme.Ses_Seviyesi"));
                	}
                    Oyuncu.addPotionEffect(new PotionEffect(PotionEffectType.HEAL, Yapilandirma.getInt("Iyilestirme.Etki_Suresi"), Yapilandirma.getInt("Iyilestirme.Etki_Gucu")));
                	}
        			else {
        				Oyuncu.sendMessage(DilDosyasi.On_Ek.toString() + DilDosyasi.Yetkiniz_Yok_Mesaji);
        				return true;
        			}
                }
            }
            if (Arguman.length == 1) {
                long Gece_Gorus_Bekleme_Suresi;
                Gece_Gorus_Bekleme_Suresi = Yapilandirma.getInt("Gece_Gorus.Bekleme_Suresi");
            	if (Yapilandirma.getBoolean("Gece_Gorus.Gece_Gorus", true))
                if (Arguman[0].equalsIgnoreCase("gecegorus") || Arguman[0].equalsIgnoreCase("gg")) {
                	if (Oyuncu.hasPermission("vipplus.gecegorus")) {
